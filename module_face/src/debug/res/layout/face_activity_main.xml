<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">
    <Button
            android:id="@+id/btn_old"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="testOldTakePhoto"
            android:text="变老"/>


    <Button
            android:id="@+id/btn_baby"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="testBabyMatchResult"
            android:text="宝宝"/>

    <Button
        android:id="@+id/btn_cartoon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="testCartoonTakePhoto"
        android:text="卡通滤镜"/>


    <Button
        android:id="@+id/btn_album"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="testAlbumTakePhoto"
        android:text="魔法相册"/>

    <Button
            android:id="@+id/btn_animal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="toAnimal"
            android:text="变动物"/>
</LinearLayout>