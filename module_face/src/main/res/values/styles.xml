<resources>

    <!-- Base application theme. -->
    <style name="face_AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/face_colorPrimary</item>
        <item name="colorPrimaryDark">@color/face_colorPrimaryDark</item>
        <item name="colorAccent">@color/face_colorAccent</item>
    </style>

    <style name="face_progress_animal" parent="Widget.AppCompat.ProgressBar.Horizontal">
        <item name="android:maxHeight">@dimen/change_38px</item>
        <item name="android:minHeight">@dimen/change_38px</item>
        <item name="android:minWidth">@dimen/change_615px</item>
        <item name="android:maxWidth">@dimen/change_615px</item>
        <item name="android:thumb">@drawable/face_thumb_progress</item>
    </style>
</resources>
